{
    "title": "Ballance wiki",
    "plugins": [
        "highlight",
        "search",
        "-sharing",

        "alerts",
        "page-footer-ex",
        "anchor-navigation-ex",
        "favicon",
        "expandable-chapters-small",
        "modal-youtube-video",
        "sectionx",
        "chinese-video",
        "sharing-plus"
    ],
    "pluginsConfig": {
        "page-footer-ex": {
            "copyright": "Text is available under the [CC-BY-SA 4.0 License](https://creativecommons.org/licenses/by-sa/4.0/).",
            "markdown": true,
            "update_label": "Last updated at: ",
            "update_format": "MM/DD/YYYY HH:mm:ss"
        },
        "favicon": {
            "shortcut": "../assets/favicon.ico",
            "bookmark": "../assets/favicon.ico",
            "appleTouch": "../assets/favicon.png",
            "appleTouchMore": {
                "120x120": "../assets/favicon.png",
                "180x180": "../assets/favicon.png"
            }
        },
        "sectionx": {
            "tag": "b"
        },
        "pluginsConfig": {
            "sharing": {
                "douban": false,
                "facebook": true,
                "google": true,
                "hatenaBookmark": false,
                "instapaper": false,
                "line": false,
                "linkedin": false,
                "messenger": false,
                "pocket": false,
                "qq": true,
                "qzone": true,
                "stumbleupon": false,
                "twitter": true,
                "viber": false,
                "vk": false,
                "weibo": true,
                "whatsapp": false,
                "all": [
                    "facebook", "google", "twitter",
                    "weibo", "qq", "qzone"
                ]
            }
        }
        
    }
}